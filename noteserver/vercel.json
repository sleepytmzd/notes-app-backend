{
    "builds": [
      {
        "src": "noteserver/wsgi.py",
        "use": "@vercel/python",
        "config": { 
          "maxLambdaSize": "15mb", 
          "runtime": "python3.9",
          "installCommand": "pip install -r requirements.txt" 
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "noteserver/wsgi.py"
      }
    ]
  }